{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "/((?!api|src|node_modules|@vite|@react-refresh|assets|data|favicon.ico).*)",
      "destination": "/index.html"
    }
  ]
}